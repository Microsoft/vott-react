{
  "name": "vott-react",
  "version": "0.1.0",
  "private": true,
  "main": "build/lib/components/index.js",
  "types": "build/lib/components/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@storybook/addon-actions": "^4.1.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-react-app": "^7.0.0",
    "react-scripts-ts": "^3.1.0",
    "react-tag-input": "^6.1.0",
    "reactstrap": "^7.1.0",
    "ts-loader": "^5.3.3",
    "node-sass-chokidar": "^1.3.4"
  },
  "scripts": {
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-sass-to-css start-js",
    "compile": "tsc",
    "build-js": "react-scripts-ts build",
    "build-lib": "npm-run-all -p build-sass-to-css build-js",
    "build": "tsc && npm run build-sass-to-css && npm run copy-css-to-lib",
    "install": "npm run build",
    "pretest": "./node_modules/.bin/tslint 'src/**/*.ts*'",
    "lintfix": "./node_modules/.bin/tslint 'src/**/*.ts*' --fix",
    "test": "react-scripts-ts test --env=jsdom --silent",
    "test:ci": "cross-env CI=true npm run test",
    "test:coverage": "npm run test -- --coverage",
    "eject": "react-scripts-ts eject",
    "copy-css-to-lib": "npm-run-all -s copy-base-css copy-component-css",
    "copy-base-css": "cpx \"./src/**/*.css\" ./build/lib/",
    "copy-component-css": "cpx \"./src/components/**/*.css\" ./build/lib/components",
    "build-sass-to-css": "node-sass-chokidar src/ -o src/",
    "watch-sass-to-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "peerDependencies": {
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0"
  },
  "devDependencies": {
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0",
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@storybook/addon-info": "^4.1.9",
    "@storybook/react": "^4.1.9",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "23.3.9",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-jsonschema-form": "^1.0.12",
    "@types/storybook__react": "^4.0.0",
    "add": "^2.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-transform": "^3.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-jsonschema-form": "^1.0.6",
    "react-modal": "^3.6.1",
    "typescript": "^3.2.2"
  },
  "files": [
    "build/lib"
  ],
  "resolutions": {
    "@types/react": "^16.7.20",
    "babel-core": "7.0.0-bridge.0"
  }
}
